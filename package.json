{
  "name": "trackly-desktop",
  "description": "Trackly Desktop Application",
  "license": "UNLICENSED",
  "version": "1.0.32",
  "homepage": "https://trackly.com",
  "author": "Trackly",
  "private": true,
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "build": "yarn build-mac && yarn build-win",
    "build-mac": "yarn compile && electron-builder --mac --p always",
    "build-win": "yarn compile && electron-builder --win --x64 --ia32 --p always",
    "postinstall": "electron-rebuild"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://trackly.com/desktop-application/"
      }
    ],
    "appId": "com.trackly.app",
    "productName": "Trackly",
    "mac": {
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "perMachine": true,
      "installerLanguages": "en_US"
    }
  },
  "iohook": {
    "targets": [
      "node-59",
      "electron-54"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "1.7.10"
  },
  "dependencies": {
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@types/electron-store": "^1.3.0",
    "@types/request": "^2.47.0",
    "active-window": "https://gitlab.trackly.com/trackly/active-window.git",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.1",
    "desktop-idle": "git+https://github.com/CudoVentures/node-desktop-idle.git#prebuild",
    "desktop-screenshot": "https://github.com/jhotujec/node-desktop-screenshot",
    "dotenv": "^5.0.1",
    "electron-is-dev": "0.3.0",
    "electron-log": "^2.2.14",
    "electron-store": "^1.3.0",
    "electron-unhandled": "^1.1.0",
    "electron-updater": "^2.21.0",
    "express": "^4.16.3",
    "iohook": "https://gitlab.trackly.com/trackly/iohook.git",
    "jquery": "^3.3.1",
    "jsonfile": "^4.0.0",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.2",
    "node-forge": "^0.7.5",
    "popper.js": "^1.14.1",
    "request": "^2.83.0",
    "rxjs": "^5.5.0",
    "source-map-support": "^0.5.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/fs-extra": "^5.0.1",
    "@types/jsonfile": "^4.0.1",
    "@types/node": "^8.0.46",
    "angular2-template-loader": "^0.6.2",
    "electron": "1.7.10",
    "electron-builder": "^20.2.0",
    "electron-rebuild": "^1.7.3",
    "electron-webpack": "^1.9.0",
    "electron-webpack-ts": "^1.1.3",
    "node-cypher": "^0.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "spectron": "^3.8.0",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1",
    "webpack-sources": "1.0.1"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  }
}
